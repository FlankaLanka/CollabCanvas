# AWS AI Agent Environment Variables

# Required: OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Allowed CORS origins (comma-separated)
ALLOWED_ORIGINS=https://collab-canvas-virid.vercel.app,http://localhost:5173,http://localhost:3000

# Optional: Node environment
NODE_ENV=production

# Optional: Server port (for EC2 deployment)
PORT=3000

# LangSmith Configuration (for AI tracing)
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=aws-ai-agent
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
